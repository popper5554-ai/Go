{
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port 8000",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/refresh?password=oxy_admin_123",
      "schedule": "0 * * * *"
    }
  ]
}
